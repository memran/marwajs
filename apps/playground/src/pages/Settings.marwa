<template>
  <h2>Settings</h2>
  <Toggler :on="isOn" @toggle="onToggle" />
  <p>Settings Status: {{ isOn.value ? 'ON' : 'OFF' }}</p>
</template>
<script lang="ts">
  const isOn = ref(false);

  function onToggle(next) { 
    console.log('toggle payload:', next); 
    isOn.value = next;
  }

</script>